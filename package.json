{
    "name": "typography",
    "version": "0.1.0",
    "description": "Type utilities for Typescript",
    "main": "out/index.js",
    "files": [
        "out"
    ],
    "exports": {
        ".": {
            "default": "./out/index.js",
            "types": "./out/index.d.ts"
        }
    },
    "scripts": {
        "clear": "npm run clear:build && npm run clear:packages",
        "clear:packages": "rimraf node_modules",
        "clear:build": "rimraf build out",
        "format": "prettier . --write",
        "check": "npm run check:cspell && npm run check:ts",
        "check:cspell": "cspell .",
        "check:ts": "tsc -p ./tsconfig.json --noEmit --composite false",
        "test": "tsc -b ./tsconfig.test.json",
        "build": "tsc -b ./tsconfig.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/y-akulau/typography.git"
    },
    "keywords": [
        "typescript",
        "utilities",
        "static-types",
        "utility-types"
    ],
    "author": "Yauheni Akulau",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/y-akulau/typography/issues"
    },
    "homepage": "https://github.com/y-akulau/typography#readme",
    "devDependencies": {
        "cspell": "9.2.2",
        "prettier": "3.6.2",
        "rimraf": "6.0.1",
        "typescript": "5.9.3"
    }
}
